<!DOCTYPE html>
<html>
	<head>
		<link media="screen" type="text/css" href="<<= @url.static >>/style.css" rel="stylesheet" />
		<script src="<<= @url.static >>/js/jquery.js"></script>
		<script src="<<= @url.static >>/js/jquery-ui-1.8.23.custom.min.js"></script>
		<script src="<<= @url.static >>/js/jquery.mousewheel.js"></script>
		<script src="<<= @url.static >>/js/jquery.mCustomScrollbar.min.js"></script>
	</head>
	<body>
		<div class="header">
			<h1><a href="<<= @url.base >>">L<span class="name-irc">irc</span></a></h1>
		</div>

		<div class="core">

			<div class="terminal-outer">
				<div class="top-tabs">
					<div class="tab" name="all">Botty</div>
					<div class="tab" name="all">Derpy</div>
				</div>
				<div class="tabs">
					<div class="tab active" name="all">All</div>
					<div class="tab" name="irc">IRC</div>
					<div class="tab" name="raw_irc">Raw IRC</div>
					<div class="tab" name="botnet">Botnet</div>

					<div class="right">
						<div class="tab" name="channel">+ Ch</div>
						<div class="tab" name="dialog">+ Di</div>
					</div>
				</div>
				<div class="terminal">
					<div class="content">
						<ul class="tab-content active" name="all"></ul>
						<ul class="tab-content" name="irc"></ul>
						<ul class="tab-content" name="raw_irc"></ul>
						<ul class="tab-content" name="botnet"></ul>
					</div>
					<div class="input">
						<span class="caret">&gt;&gt;</span><input name="terminal-input" value=""/>
					</div>
				</div>
			</div>
		</div>

		<div class="footer">
			<h2>Commands...</h2>
			<ul>
				<li>.join</li>
			</ul>
		</div>
	</body>
	<script src="<<= @url.static >>/js/socket.io.js"></script>
	<script src="<<= @url.static >>/app.js"></script>
</html>